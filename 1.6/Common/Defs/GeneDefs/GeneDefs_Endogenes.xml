<?xml version="1.0" encoding="UTF-8" ?>

<Defs>
    
    <!-- Headbone -->
    <GeneDef ParentName="GeneHeadboneBase">
        <defName>Turn_Gene_HeadboneCrown</defName>
        <label>crown of horns</label>
        <labelShortAdj>crown</labelShortAdj>
        <description>Carriers of this gene grow a "crown" of horns above their brow.</description>
        <iconPath>UI/Icons/Genes/Gene_HeadboneCrown</iconPath>
        <renderNodeProperties>
            <li>
                <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
                <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
                <texPath>Things/Pawn/Humanlike/HeadAttachments/CrownHorns/CrownHorns</texPath>
                <color>(255, 255, 255)</color>
                <parentTagDef>Head</parentTagDef>
                <drawData>
                    <defaultData>
                        <layer>80</layer>
                    </defaultData>
                    <dataNorth>
                        <layer>10</layer>
                    </dataNorth>
                </drawData>
            </li>
        </renderNodeProperties>
        <symbolPack>
            <prefixSymbols>
                <li><symbol>horn</symbol></li>
                <li><symbol>spike</symbol></li>
                <li><symbol>roy</symbol></li>
                <li><symbol>reg</symbol></li>
                <li><symbol>crown</symbol></li>
            </prefixSymbols>
            <suffixSymbols>
                <li><symbol>horn</symbol></li>
                <li><symbol>spike</symbol></li>
                <li><symbol>crown</symbol></li>
            </suffixSymbols>
        </symbolPack>
    </GeneDef>
</Defs>